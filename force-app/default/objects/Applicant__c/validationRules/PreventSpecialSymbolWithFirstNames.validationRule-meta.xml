<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PreventSpecialSymbolWithFirstNames</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX (First_Name__c, &quot;^[a-zA-Z0-9_]*$&quot;))</errorConditionFormula>
    <errorDisplayField>First_Name__c</errorDisplayField>
    <errorMessage>Prevent special  symbol................................</errorMessage>
</ValidationRule>

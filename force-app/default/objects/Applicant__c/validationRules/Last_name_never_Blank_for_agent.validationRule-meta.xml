<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Last_name_never_Blank_for_agent</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISBLANK( Last_Name__c), $Profile.Name  = &apos;Agent&apos;)</errorConditionFormula>
    <errorMessage>Last name cannot be empty for Agent profile</errorMessage>
</ValidationRule>
